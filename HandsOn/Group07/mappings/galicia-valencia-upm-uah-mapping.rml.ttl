@prefix lude: <http://spanishuniversities.data.es/lude/ontology#>.
@prefix id: <http://spanishuniversities.data.es/lude/resource/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix schema: <http://schema.org/>.
@prefix formats: <http://www.w3.org/ns/formats/>.
@prefix comp: <http://semweb.mmlab.be/ns/rml-compression#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@base <http://example.com/ns#>.


<DegreeMapping_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "galicia-valencia-upm-uah-titulaciones-updated.csv";
		rml:referenceFormulation ql:CSV

	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://spanishuniversities.data.es/lude/resource/Degree/{DegreeID}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant lude:Degree;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdfs:label;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Degree";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:academicYear;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "AcademicYear";
			rr:datatype xsd:string
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:code;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Code";
			rr:datatype xsd:string
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasAcademicLevel;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <AcademicLevelMapping_0>;
			rr:joinCondition [
				rr:child "AcademicLevelID";
				rr:parent "AcademicLevelID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasKnowledgeArea;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <KnowledgeAreaMapping_0>;
			rr:joinCondition [
				rr:child "AmbitId";
				rr:parent "id";
			];
		];
	].


