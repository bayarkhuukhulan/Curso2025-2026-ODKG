<DegreeCastillaLeonMapping_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "data/CASTILLALEON-degree.csv";
		rml:referenceFormulation ql:CSV

	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "https://linkeduniversities.es/resource/Degree/{DegreeID}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant lude:Degree;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdfs:label;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Degree";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:academicYear;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Academic Year";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasAcademicLevel;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <AcademicLevelMapping_0>;
			rr:joinCondition [
				rr:child "AcademicLevelID";
				rr:parent "AcademicLevelID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasKnowledgeArea;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <KnowledgeAreaMapping_0>;
			rr:joinCondition [
				rr:child "AmbitID";
				rr:parent "id";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:deliveredAt;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <FacultyMappingCastillaLeon_0>;
			rr:joinCondition [
				rr:child "FacultyID";
				rr:parent "FacultyID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:offers;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <UniversityMapping_0>;
			rr:joinCondition [
				rr:child "FacultyID";
				rr:parent "FacultyID";
			];
		];
	].


