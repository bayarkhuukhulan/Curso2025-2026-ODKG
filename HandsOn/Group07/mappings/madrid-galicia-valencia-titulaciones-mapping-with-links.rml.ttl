<DegreeMapping_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "galicia-valencia-upm-uah-titulaciones-updated-with-links.csv";
		rml:referenceFormulation ql:CSV

	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://spanishuniversities.data.es/lude/resource/Degree/{DegreeID}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdf:type;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rr:constant lude:Degree;
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdfs:label;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Degree";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:academicYear;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Academic Year";
			rr:datatype xsd:string
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:code;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Code";
			rr:datatype xsd:string
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant owl:sameAs;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "same_as_wikidata_degree";
			rr:termType rr:IRI
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasAcademicLevel;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <AcademicLevelMapping_0>;
			rr:joinCondition [
				rr:child "AcademicLevelID";
				rr:parent "AcademicLevelID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant lude:hasKnowledgeArea;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <KnowledgeAreaMapping_0>;
			rr:joinCondition [
				rr:child "AmbitId";
				rr:parent "id";
			];
		];
	].


